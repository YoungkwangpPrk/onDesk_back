<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.com4in.itsm.mapper.SapTokenInfoMapper">

    <select id="getSapTokenInfo" resultType="com.com4in.itsm.dto.SapTokenInfoDto">
        select
            *
        FROM
            SAP_TOKEN_INFO
        WHERE
            companyId=#{companyId}
    </select>

</mapper>